{
  "cloudURL": "<<cloud name>>",
  "securityToken": "<<SECURITY TOKEN>>",
  "appPath": "Test.app",
  "testAppPath": "TestUITests-Runner.app",
  "devices": [
    {
      "model" : "iPhone.*",
      "platformVersion": "16.*|17.*"
    }
  ],
  "tags": [
    "xcuitest",
    "sample"
  ],
  "projectName": "IOS-XCUITest",
